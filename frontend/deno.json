{
  "tasks": {
    "dev": "deno run -A npm:vite",
    "build": "deno run -A npm:vite build",
    "preview": "deno run -A npm:vite preview",
    "lint": "deno lint"
  },
  "imports": {
    "react": "npm:react@^19.2.0",
    "react-dom": "npm:react-dom@^19.2.0",
    "react-dom/client": "npm:react-dom@^19.2.0/client",
    "react/jsx-runtime": "npm:react@^19.2.0/jsx-runtime",
    "@types/react": "npm:@types/react@^19.2.0",
    "vite": "npm:vite@^6.3.5",
    "@vitejs/plugin-react-swc": "npm:@vitejs/plugin-react-swc@^4.2.2",
    "tailwindcss": "npm:tailwindcss@^4.1.18",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.1.18"
  },
  "compilerOptions": {
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "nodeModulesDir": "auto"
}
